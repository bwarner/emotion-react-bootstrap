{
  "presets": [
    "react",
    [
      "env",
      {
        "modules": false,
        "targets": {
          // The % refers to the global coverage of users from browserslist
          "browsers": [ ">5%", "not ie 11", "not op_mini all"]
        }
      }
    ],
    "stage-0"
  ],
  "ignore": [
  ],
  "plugins": [
    ["emotion", { "sourceMap": true, "autoLabel": true }],
    "syntax-class-properties"
  ],
  "env": {
    "test": {
      "plugins": [
        "require-context-hook"
      ]
    }
  }
}
