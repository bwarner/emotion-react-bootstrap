{
  "presets": [
    "react",
    [
      "env",
      {
        "modules": false,
        "targets": {
          // The % refers to the global coverage of users from browserslist
          "browsers": [ ">5%", "not ie 11", "not op_mini all"]
        }
      }
    ]
  ],
  "ignore": [
    "node_modules"
  ],
  "plugins": [
    "transform-class-properties",
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ]
}
